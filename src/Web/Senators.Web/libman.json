{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "vue@2.6.2",
      "destination": "wwwroot/lib/vuejs"
    },
    {
      "provider": "unpkg",
      "library": "@aspnet/signalr@1.1.0",
      "destination": "wwwroot/lib/@aspnet/signalr/"
    },
    {
      "library": "toastr.js@2.1.4",
      "destination": "wwwroot/lib/toastr.js/",
      "files": [
        "toastr.min.js",
        "toastr.css",
        "toastr.js.map",
        "toastr.min.css"
      ]
    }
  ]
}